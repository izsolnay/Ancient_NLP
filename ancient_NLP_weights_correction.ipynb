    "# Define and remove stopwords\n",
    "# Create loop, because discard takes only 1 argument\n",
    "stop_words = set(stopwords.words('english'))\n",
    "\n",
    "# Create a set of custom stop words\n",
    "custom_stopwords = {'-……',\n",
